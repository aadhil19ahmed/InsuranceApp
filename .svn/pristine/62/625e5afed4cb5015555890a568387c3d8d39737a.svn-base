using System;
using System.Collections.Generic;
using ACIA.Views.UILayer;

namespace ACIA.Model
{
    public class DynamicPageModel
    {
        /// <summary>
        /// Initializes a new instance of the <see cref="T:DynamicUI.DynamicPageModel"/> class.
        /// </summary>
        public DynamicPageModel()
        {
            Sections = new List<BaseSection>();
            Views = new List<BaseSectionView>();
        }

        public string Pagetype { get; set; }
        public string Screenname { get; set; }
        public string NavigationBarColor { get; set; }
        public string NavigationTextColor { get; set; }
        public List<BaseSection> Sections { get; set; }
        public string name { get; set; }
        public string image { get; set; }
        public string url { get; set; }
        public string dataurl { get; set; }
        public string viewtype { get; set; }
        public List<BaseSectionView> Views { get; set; }
        public string Error { get; set; }
        public string status { get; set; }

    }
}
